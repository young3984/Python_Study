   "source": [
    "n,m=map(int,input().split(':'))\n",
    "a=max(n,m)\n",
    "b=min(n,m)\n",
    "while(1):\n",
    "    if a%b==0:\n",
    "        break\n",
    "    c=a%b\n",
    "    a=b\n",
    "    b=c\n",
    "print(\"%d:%d\"%(n//b,m//b))"
   ]
