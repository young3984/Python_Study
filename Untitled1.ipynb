      "1\n",
      "aabcbb\n",
      "0\n"
     ]
    }
   ],
   "source": [
    "N=int(input())\n",
    "ans=N\n",
    "for ik in range(N):\n",
    "    word=list(input())\n",
    "    A=[word[0]]\n",
    "    a=word[0]\n",
    "    for it in range(1,len(word)):\n",
    "        if word[it]!=a:\n",
    "            if word[it] in A:\n",
    "                ans-=1\n",
    "                break\n",
    "            else:\n",
    "                a=word[it]\n",
    "                A.append(word[it])\n",
    "print(ans)"
   ]
