    "N=int(input())\n",
    "arr=list(map(int,input().split()))\n",
    "from itertools import permutations\n",
    "S=list(set(permutations(arr, N)))\n",
    "ans=[]\n",
    "for ik in S:\n",
    "    s=[0]\n",
    "    t=0\n",
    "    for it in range(N):\n",
    "        s.append(ik[it]+s[-1])\n",
    "    for it in range(1,N):\n",
    "        if (s[it]+50) in s:\n",
    "            t+=1\n",
    "    ans.append(t)\n",
    "print(max(ans))"
   ]
